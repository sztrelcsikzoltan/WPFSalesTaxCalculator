<Window x:Class="WPFSalesTaxCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFSalesTaxCalculator"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Sales Tax Calculator" Height="600" Width="800">
    <Grid>
        <StackPanel Margin="0,0,0,292">
            <RichTextBox Name="richTextBox" FontSize="15" Block.LineHeight="2" Margin="31,40,212,292" Height="210" TextChanged="richTextBox_TextChanged">
            </RichTextBox>
        </StackPanel>
    </Grid>
</Window>
